            "comment": "执教委托事件控制",
            "content": "@@preprocessing\n<%_\n/* 声明变量 */\nif (typeof currentLocation === 'undefined') var currentLocation = getvar('stat_data.世界.当前地点', { defaults: '' });\nif (typeof level === 'undefined') var level = getvar('stat_data.主角.属性.等级', { defaults: 0 });\nif (typeof groupConnections === 'undefined') var groupConnections = getvar('stat_data.主角.人脉.团体', { defaults: {} });\n\n/* 辅助函数：检查单个团体是否满足条件 (存在且关系度>3) */\n/* 注意：为了代码简洁，直接在if中展开判断 */\n_%>\n\n<%_\n/* 逻辑判断部分 */\n/* 条件1：位于神魔智学园 */\nvar isAtSchool = currentLocation.includes('神魔智学园');\n\n/* 条件2：等级 >= 4 */\nvar isLevelEnough = (level >= 4);\n\n/* 条件3：特定团体人脉关系度 > 3 */\n/* 检查：悦乐同盟、炼狱斗技场、伊什塔尔神殿、大江山酒窖、克洛塞尔大浴场、神魔智学园 */\nvar hasConnection = false;\n\nif (typeof groupConnections['悦乐同盟'] !== 'undefined' && groupConnections['悦乐同盟'].关系度 > 3) hasConnection = true;\nif (typeof groupConnections['炼狱斗技场'] !== 'undefined' && groupConnections['炼狱斗技场'].关系度 > 3) hasConnection = true;\nif (typeof groupConnections['伊什塔尔神殿'] !== 'undefined' && groupConnections['伊什塔尔神殿'].关系度 > 3) hasConnection = true;\nif (typeof groupConnections['大江山酒窖'] !== 'undefined' && groupConnections['大江山酒窖'].关系度 > 3) hasConnection = true;\nif (typeof groupConnections['克洛塞尔大浴场'] !== 'undefined' && groupConnections['克洛塞尔大浴场'].关系度 > 3) hasConnection = true;\nif (typeof groupConnections['神魔智学园'] !== 'undefined' && groupConnections['神魔智学园'].关系度 > 3) hasConnection = true;\n_%>\n\n<%_ /* 最终判定：位于学园 AND (等级够 OR 有人脉) */ _%>\n<%_ if (isAtSchool && (isLevelEnough || hasConnection)) { _%>\n执教委托事件\n<%_ } _%>",
